{
  "compilerOptions": {
      "target": "es6",
      "module": "es2020",
      "moduleResolution": "node",
      "outDir": "./build",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
  },
  "include": ["src/backend/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}